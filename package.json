{
  "name": "riot-webpack",
  "version": "0.0.1",
  "main": "app/main.js",
  "bonito": ">=3.102.7",
  "ca": ">=20170802",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --progress --colors -d --port 3001 --disable-host-check --https",
    "dev": "webpack-dev-server --config webpack.dev.js -d --port 3001 --inline=false",
    "build": "webpack --config webpack.prod.js --output-filename bundle.js --output-path .",
    "stats": "webpack-dev-server --config webpack.stats.js --progress",
    "scss": "node-sass -rw ./app/styles/bundle.scss -o ./app/styles/ --output-style compressed"
  },
  "devDependencies": {
    "browser-update": "^3.3.9",
    "css-loader": "^1.0.1",
    "riot": "^3.13.0",
    "riot-compiler": "^3.5.1",
    "riot-hot-reload": "1.0.0",
    "riot-tag-loader": "^2.1.0",
    "sass": "^1.54.4",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "d3-array": "^2.0.3",
    "d3-axis": "^1.0.8",
    "d3-color": "^1.2.3",
    "d3-ease": "^1.0.5",
    "d3-force": "^2.0.1",
    "d3-request": "^1.0.6",
    "d3-scale": "^3.0.0",
    "d3-selection": "^1.4.0",
    "d3-shape": "^1.3.5",
    "d3-transition": "^1.2.0",
    "input-range-scss": "1.5.2",
    "intro.js": "^2.9.3",
    "kld-intersections": "^0.4.3",
    "lodash": "^4.17.11",
    "node-polyglot": "^2.4.0",
    "riot-route": "^3.1.4"
  }
}
